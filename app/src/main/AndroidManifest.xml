<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dhisa.example.baseapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityTambahData"
            android:label="@string/tambah_activity"/>
        <activity
            android:name=".ActivityDashboard"
            android:label="@string/first_activity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".ActivityResponden"
            android:label="@string/second_activity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ActivityRelawan"
            android:label="@string/third_activity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ActivitySettings"
            android:label="@string/fourth_activity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ActivityLaporan"
            android:label="@string/fifth_activity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>